{
	"name":"MODS SAES",
	"version": "0.53",
	"manifest_version": 2,
	"description": "__MSG_description__",
    "minimum_chrome_version": "18",
	"default_locale":"es",
	"icons": {
      	"16": "/css/fu_16.png",
      	"48": "/css/fu_48.png",
      	"128": "/css/fu_128.png"
    },
	"permissions": [ "tabs", "*://*.ipn.mx/*", "*://*.google.com/*"],
	"homepage_url": "http://foroupiicsa.net/extension-saes/",
	"options_page":"opciones.html",
    "background":{
    	"scripts": ["segundoPlano.js"],
    	"persistent": false
    },
    "web_accessible_resources": ["css/menos.png","css/1.jpg","css/2.jpg"],
	"content_scripts": [
		{
			"matches" : ["*://*.ipn.mx/PDF/Alumnos/Reinscripciones/*"],
			"js" : ["sec.js"],
			"run_at" : "document_start"
		},{
			"matches" : ["*://*.ipn.mx/*"],
			"css": ["estilo.css"],
			"js" : ["jsX.js"],
			"run_at" : "document_end"
		}
	]
}
